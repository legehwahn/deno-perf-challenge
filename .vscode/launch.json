{
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Debug Runtime",
			"runtimeExecutable": "deno",
			"runtimeArgs": [
				"run",
				"--inspect-brk",
				"--allow-all",
				"--unstable",
				"mod.ts"
			],
			"env": {
				"PORT": "3000"
			},
			"cwd": "${workspaceFolder}",
			"attachSimplePort": 9229,
			"autoAttachChildProcesses": true
		}
	]
}